{
  // https://nuxt.com/docs/guide/concepts/typescript
  "compilerOptions": {
    "types": [
      "vue3-text-clamp"
    ]
  },
  "extends": "./.nuxt/tsconfig.json",
  "include": [
    "types/**/*.d.ts",
    "nuxt.config.ts",
    "app.vue",
    "components/**/*.vue",
    "pages/**/*.vue",
    "plugins/**/*.ts",
    "layouts/**/*.vue"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
